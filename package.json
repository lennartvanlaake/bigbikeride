{
	"name": "svelte-app",
	"version": "0.4.24",
	"type": "commonjs",
	"scripts": {
		"deploy-be": "yarn version --patch && bash deploy-be.sh $npm_package_version",
		"deploy-fe": "yarn version --patch && bash deploy-fe.sh $npm_package_version",
		"deploy-migration": "bash migrate.sh",
		"cucumber": "sudo docker-compose up -d && cd backend && cucumber-js --profile default",
		"cucumber-debug": "sudo docker-compose up -d && cd backend && node --inspect-brk ../node_modules/.bin/cucumber-js --profile default",
		"start-fe": "cd frontend && vite",
		"start-be": "sudo docker-compose up -d && cd backend && nodemon",
		"make-migration": "knex --knexfile backend/knexfile.ts migrate:make -x ts",
		"migrate": "knex --knexfile backend/knexfile.ts migrate:latest",
		"start": "yarn start-fe & yarn start-be"
	},
	"devDependencies": {
		"@types/cors": "^2.8.10",
		"@types/koa": "^2.13.1",
		"@types/koa-bodyparser": "^4.3.0",
		"@types/koa-json": "^2.0.18",
		"@types/koa-logger": "^3.1.1",
		"@types/koa-router": "^7.4.1",
		"@types/koa-session": "^5.10.3",
		"@types/koa-static": "^4.0.1",
		"@types/koa__cors": "^3.0.2",
		"@types/koa__multer": "^2.0.2",
		"@types/leaflet": "^1.7.0",
		"@types/marked": "^2.0.1",
		"@types/multer": "^1.4.5",
		"@types/nodemailer": "^6.4.4",
		"@types/nodemon": "^1.19.0",
		"@types/page": "^1.11.2",
		"@types/pg": "^7.14.11",
		"@types/supertest": "^2.0.11",
		"@types/uuid": "^8.3.0",
		"svelte": "^3.0.0"
	},
	"dependencies": {
		"@cucumber/cucumber": "^7.1.0",
		"@koa/cors": "^3.1.0",
		"@koa/multer": "^3.0.0",
		"@snowpack/plugin-dotenv": "^2.0.5",
		"@snowpack/plugin-typescript": "^1.2.1",
		"@sveltejs/svelte-virtual-list": "^3.0.1",
		"@sveltejs/vite-plugin-svelte": "^1.0.0-next.9",
		"@tsconfig/svelte": "^1.0.10",
		"@types/chai": "^4.2.16",
		"@types/cucumber": "^7.0.0",
		"@types/express": "^4.17.11",
		"@types/express-session": "^1.17.3",
		"@types/node": "^14.14.37",
		"autoprefixer": "^10.2.4",
		"axios": "^0.21.1",
		"chai": "^4.3.4",
		"cors": "^2.8.5",
		"dotenv": "^8.2.0",
		"filepond": "^4.25.1",
		"filepond-plugin-image-preview": "^4.6.5",
		"knex": "^0.95.4",
		"koa": "^2.13.1",
		"koa-bodyparser": "^4.3.0",
